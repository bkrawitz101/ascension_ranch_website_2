{
  "name": "ascension-ranch-site",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:gh": "BUILD_TARGET=gh npm run build",
    "build:prod": "BUILD_TARGET=prod npm run build",
    "postbuild": "cp -f dist/index.html dist/404.html",
    "preview": "vite preview --port 4173",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:gh": "npm run build:gh && gh-pages -d dist",
    "backup": "bash ./scripts/backup.sh",
    "backup:full": "bash ./scripts/backup.sh --full"
  },
  "dependencies": {
    "lucide-react": "^0.259.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.4.19",
    "vite": "^7.3.0"
  }
}
